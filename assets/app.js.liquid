var banner_image = {
  init: function(){

    if ($(window).width() > 798) {

      if ($('body').hasClass('index') || $('.shopify-section.page-details-section').length){

        var $reorderableFirstSection = $('.index-sections .shopify-section, .page-details-section .page-details-block').first();

